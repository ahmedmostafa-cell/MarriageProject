@model MarriageProject.Models.HomePageModel
<!-- Content Header (Page header) -->
<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>DataTables</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">DataTables</li>
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>

<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-12">


            <div class="card">
                <div class="card-header">
                    <h3 class="card-title">DataTable with default features</h3>
                </div>
                <!-- /.card-header -->
                <div class="card-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>اسم المستخدم</th>
                                <th>العائلة</th>
                                <th>السن</th>
                                <th>الصورة الشخصية</th>
                                <th>المؤهل</th>
                                <th>رقم التليفون</th>
                               
                                <th>الحالة الاجتماعية</th>
                               
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.lstNormalUserS)
                            {

                                <tr>
                                    <td>@item.NormalUserName</td>
                                    <td>
                                        @item.NormalUserFamilyName
                                    </td>
                                    <td>@item.NormalUserAge</td>
                                    <td>
                                        <img style="width:100px; height:100px" src="@string.Format("/Uploads/{0}",item.NormalUserImage)" alt="Img placeholder">
                                    </td>
                                    <td> @item.NormalUserCertificate</td>
                                    <td>@item.NormalUserPhoneNo</td>
                                    <td>@item.NormalUserMaritalStatus</td>
                                   
                                </tr>

                            }


                        </tbody>
                        <tfoot>
                            <tr>
                                <th>Rendering engine</th>
                                <th>Browser</th>
                                <th>Platform(s)</th>
                                <th>Engine version</th>
                                <th>CSS grade</th>
                            </tr>
                        </tfoot>
                    </table>
                </div>
                <!-- /.card-body -->
            </div>
            <!-- /.card -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>
<!-- /.content -->
